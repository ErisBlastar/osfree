#ifndef om_idl
#define om_idl
#include <somobj.idl>
#include <somdtype.idl>
#include <snglicls.idl>
#include <containd.idl>
interface ObjectMgr :
	SOMObject
{
	SOMObject somdNewObject(
		in Identifier objclass,
		in string hints);
	string somdGetIdFromObject(
		in SOMObject obj);
	SOMObject somdGetObjectFromId(
		in string id);
	void somdReleaseObject(
		in SOMObject obj);
	void somdDestroyObject(
		in SOMObject obj);
#ifdef __SOMIDL__
	implementation
	{
		releaseorder : 
			somdNewObject,
			somdGetIdFromObject,
			somdGetObjectFromId,
			somdReleaseObject,
			somdDestroyObject;
		callstyle = idl;
/*		metaclass = SOMMSingleInstance;*/
		dllname = "somd.dll";
		majorversion = 2;
		minorversion = 2;
	};
#endif /* __SOMIDL__ */
};
#endif /* om_idl */
