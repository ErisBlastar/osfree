module $(PN)/fstab ^
  -c /file ^
  -c /file/system -v 14 -b / -m /file/system ^
  -c /dev -v 132 -b / -m /dev
module $(PN)/fprov_proxy
module $(PN)/loader --fprov=fprov_proxy_fs --events /file/system/run.cfg /file/system/os2.cfg
