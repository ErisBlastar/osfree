;
; variables, specific to
; prokushev's configuration
; (in REXX notation, except
; for comments)
;

; Watcom root
watcom  = '/media/watcom'
; osFree sources root
root    = '/media/osfree'
; disk images directory
imgdir  = '/media'
; OS/2 toolkit location
tkpath  = ''
; Free Pascal directory
fppath  = '/usr/bin'
; Shell
shell   = 'sh'
; REXX interpreter
rexx    = 'rexx'
rexx_path    = ''
; mkisofs
mkisofs = 'c:\osfree\cdrtoolsw32\mkisofsw32.exe'
; os2server host
serverenv = 'linux'
os = 'LINUX'

