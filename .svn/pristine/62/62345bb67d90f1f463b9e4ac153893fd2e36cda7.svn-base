#ifndef somdcprx_idl
#define somdcprx_idl
#include <somobj.idl>
#include <somproxy.idl>
#include <somdobj.idl>
interface SOMDClientProxy;
interface SOMDClientProxy :
	SOMMProxyForObject,
	SOMDObject
{
	void somdTargetFree();
	SOMClass somdTargetGetClass();
	string somdTargetGetClassName();
	void somdProxyFree();
	SOMClass somdProxyGetClass();
	string somdProxyGetClassName();
	void somdReleaseResources();
#ifdef __SOMIDL__
	implementation
	{
		releaseorder : 
			somdTargetFree,
			somdTargetGetClass,
			somdTargetGetClassName,
			somdProxyFree,
			somdProxyGetClass,
			somdProxyGetClassName,
			private6,
			private7,
			private8,
			private9,
			private10,
			somdReleaseResources,
			private12,
			private13,
			private14,
			private15,
			private16;
		callstyle = idl;
		dllname = "somd.dll";
		majorversion = 2;
		minorversion = 2;
	};
#endif /* __SOMIDL__ */
};
#endif /* somdcprx_idl */
