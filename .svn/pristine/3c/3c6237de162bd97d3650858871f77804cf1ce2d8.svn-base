     MENUITEM    "~Keys help",           IDM_HELPKEYS,       MIS_TEXT
     MENUITEM    "~Using help",          IDM_HELPUSINGHELP,  MIS_TEXT
     MENUITEM SEPARATOR
     MENUITEM    "~Product information", IDM_HELPPRODINFO,   MIS_TEXT
  }
}

