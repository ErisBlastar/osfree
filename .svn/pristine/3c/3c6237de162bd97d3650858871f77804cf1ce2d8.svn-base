         MENUITEM    "~Cyan",    IDM_OPTIONSBACKCOLORCYAN,   MIS_TEXT
         MENUITEM    "~Default", IDM_OPTIONSBACKCOLORDEFAULT,MIS_TEXT
      }

      MENUITEM SEPARATOR
      MENUITEM    "F~ont...\tCtrl+F",      IDM_OPTIONSFONT,     MIS_TEXT
   }

