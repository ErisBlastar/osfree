LIBRARY EXT2FLT
EXETYPE OS2
DESCRIPTION 'Linux ext2 partition filter (Alpha)'
PROTMODE
SEGMENTS
 _DATA          CLASS 'DATA'
 _BSS           CLASS 'BSS'
CONST           CLASS 'CONST'
 _TEXT          CLASS 'CODE'

