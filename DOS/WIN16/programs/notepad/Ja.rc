STRING_WARNING,                                 "警告"
STRING_INFO,                                    "情報"

STRING_UNTITLED,                                "(無題)"

STRING_ALL_FILES,                               "全てのファイル (*.*)"
STRING_TEXT_FILES_TXT,                  "テキストファイル (*.txt)"

